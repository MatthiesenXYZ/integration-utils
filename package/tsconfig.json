{
  "extends": "astro/tsconfigs/strictest",
  "files": [],
  "compilerOptions": {
    "outDir": "../.moon/cache/types/package",
    "composite": true,
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "emitDeclarationOnly": false,
    "paths": {
      "playground/*": ["../playground/*"]
    }
  },
  "references": [
    {
      "path": "../playground"
    }
  ],
  "include": [
    "./package.json",
    "./virt-dev.d.ts",
    "../playground/.astro/**/*",
    "./**/*",
    "./src/**/*.json"
  ]
}
